#include<stdio.h>
#include<string.h>
int main()
{
int n;
scanf("%d",&n);
for(int i=1;i<=n;i++)
{
    for(int j=1;j<=n;j++){
        
        printf("%c",i+64);
    }
    printf("\n");
}
return 0;
}

//another way to solve the above one

#include<stdio.h>
#include<string.h>
int main()
{
int n;
scanf("%d",&n);
for(char i='A';i<n+'A';i++)
{
    for(char j='A';j<n+'A';j++){
        
        printf("%c",i);
    }
    printf("\n");
}
return 0;
}
